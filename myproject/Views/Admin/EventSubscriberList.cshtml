@model IEnumerable<myproject.Models.EventSubscriber>

@{
    ViewBag.Title = "EventSubscriberList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Subscriber List</h2>

@using (Html.BeginForm("SendEventMail", "Community"))
{
    <div>
        <input name="youtubelink" placeholder="Youtube live link"/>
        <button type="submit">Send email for subscriber</button>
    </div>
}

<table class="table">
    <tr>
        <th>
            Subscriber Email
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SubscriberEmail)
            </td>

        </tr>
    }

</table>
